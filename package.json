{
  "name": "alemonjs-testone",
  "displayName": "ALemonTestOne",
  "publisher": "lemonadex",
  "description": "阿柠檬沙盒环境",
  "version": "0.0.7",
  "scripts": {
    "dev": "vite",
    "bundle": "tsc && NODE_ENV='development' vite build",
    "build": "tsc && vite build",
    "eslint": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.x",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vscode/test-cli": "^0.0.10",
    "alemonjs": "^2.1.0-alpha.2",
    "animate.css": "^4.1.1",
    "antd": "^5.26.7",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "flatted": "^3.3.3",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "sass": "^1.75.0",
    "tailwindcss": "^3.4.3",
    "terser": "^5.30.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonade-lab/alemontestone.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/favicon.ico",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ALemonTestOne.openGroup",
        "title": "群聊",
        "icon": "$(organization)"
      },
      {
        "command": "ALemonTestOne.openPrivate",
        "title": "私聊",
        "icon": "$(person)"
      },
      {
        "command": "ALemonTestOne.openPanel",
        "title": "新建窗口"
      },
      {
        "command": "ALemonTestOne.openSidebar",
        "title": "侧边栏"
      },
      {
        "command": "ALemonTestOne.openConnect",
        "title": "切换连接"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "alemonjs",
          "title": "AlemonJS",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "alemonjs": [
        {
          "type": "webview",
          "id": "ALemonTestOne.chatSidebar",
          "name": ""
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "when": "view == ALemonTestOne.chatSidebar",
          "command": "ALemonTestOne.openGroup",
          "group": "navigation"
        },
        {
          "when": "view == ALemonTestOne.chatSidebar",
          "command": "ALemonTestOne.openPrivate",
          "group": "navigation"
        },
        {
          "when": "view == ALemonTestOne.chatSidebar",
          "command": "ALemonTestOne.openConnect"
        },
        {
          "when": "view == ALemonTestOne.chatSidebar",
          "command": "ALemonTestOne.openPanel"
        }
      ]
    }
  }
}
